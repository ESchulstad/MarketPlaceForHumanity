@model IEnumerable<GotFoodConnections.Models.Transport>


@{
    ViewBag.Title = "TransportList";
}

<h2>TransportList</h2>
<table class="table">
<tr>
    <th>
        @Html.DisplayNameFor(model => model.OrganizationName)
    </th>
    <th></th>
</tr>
@foreach (var item in Model)
{
    <tr>
        <td>
            @*@Html.DisplayFor(modelItem => item.OrganizationName)*@
            @Html.ActionLink(item.OrganizationName, "Details", new { id = item.TransportID })
        </td>
    </tr>
}

</table>


